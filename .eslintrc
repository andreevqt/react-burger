{
  "root": true,
  "extends": [
    "react-app",
    "react-app/jest",
    "airbnb"
  ],
  "rules": {
    "linebreak-style": 0,
    "dot-notation": 0,
    "no-return-assign": 0,
    "react/function-component-definition": [
      2,
      {
        "namedComponents": "arrow-function"
      }
    ],
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-alert": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/no-static-element-interactions": 0
  }
}
